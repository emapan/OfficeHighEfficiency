   "source": [
    "from scipy import poly1d\n",
    "p = poly1d([3, 4, 5]) #构建一元多次方程\n",
    "print(p)\n",
    "print(p*p)\n",
    "print(p.integ(k=6))    # 求p(x)的不定积分，指定常数项为6\n",
    "print(p.deriv())       # 求p(x)的一阶导数\n",
    "print(p([4, 5]))       # 计算每个值代入p(x)的结果"
   ]
  }
